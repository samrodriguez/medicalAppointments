<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Menu{% endblock %}</title>
        {% block css %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

    </head>
    <body>
        <br>
        <div class="container"><br>
            {% block body %}
            <div class="row">
                <div class="col-sm-8 offset-2">
                    {% block content %}

                    {% endblock %}
                </div>
            </div>
            {% endblock %}
        </div>

    {% block js %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}
    </body>
</html>
